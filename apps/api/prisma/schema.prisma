generator client {
  provider   = "prisma-client"
  output     = "./generated"
}

datasource db {
  provider = "postgresql"
}


model User {
  id           Int      @id @default(autoincrement())
  githubId     String   @unique
  username     String?
  email        String?  @unique
  avatarUrl    String?
  accessToken  String?
  createdAt    DateTime @default(now())
}
